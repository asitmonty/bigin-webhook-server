  "errorHandling": {
    "retryAttempts": 3,
    "retryDelay": 1000,
    "fallbackToDescription": true,
    "logErrors": true
  },
  "rateLimiting": {
    "maxRequestsPerMinute": 10,
    "minRequestInterval": 6000,
    "enableRateLimit": true,
    "retryOnRateLimit": true,
    "maxRetries": 3
  }
