{
  "name": "saas-template-framework",
  "version": "1.0.0",
  "description": "Reusable SaaS Template Framework with Agent System",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run dev:services\" \"npm run dev:apps\"",
    "dev:services": "concurrently \"npm run dev:auth\" \"npm run dev:customer\" \"npm run dev:payment\" \"npm run dev:subscription\" \"npm run dev:reporting\" \"npm run dev:support\" \"npm run dev:integration\" \"npm run dev:webhook\"",
    "dev:apps": "concurrently \"npm run dev:marketing\" \"npm run dev:customer-portal\" \"npm run dev:admin-portal\"",
    "dev:auth": "cd services/auth-service && npm run dev",
    "dev:customer": "cd services/customer-service && npm run dev",
    "dev:payment": "cd services/payment-service && npm run dev",
    "dev:subscription": "cd services/subscription-service && npm run dev",
    "dev:reporting": "cd services/reporting-service && npm run dev",
    "dev:support": "cd services/support-service && npm run dev",
    "dev:integration": "cd services/integration-service && npm run dev",
    "dev:webhook": "cd services/webhook-service && npm run dev",
    "dev:marketing": "cd apps/marketing-website && npm run dev",
    "dev:customer-portal": "cd apps/customer-portal && npm run dev",
    "dev:admin-portal": "cd apps/admin-portal && npm run dev",
    "build": "echo 'No build required for webhook server' || true",
    "build:services": "concurrently \"npm run build:auth\" \"npm run build:customer\" \"npm run build:payment\" \"npm run build:subscription\" \"npm run build:reporting\" \"npm run build:support\" \"npm run build:integration\" \"npm run build:webhook\"",
    "build:apps": "concurrently \"npm run build:marketing\" \"npm run build:customer-portal\" \"npm run build:admin-portal\"",
    "build:auth": "cd services/auth-service && npm run build",
    "build:customer": "cd services/customer-service && npm run build",
    "build:payment": "cd services/payment-service && npm run build",
    "build:subscription": "cd services/subscription-service && npm run build",
    "build:reporting": "cd services/reporting-service && npm run build",
    "build:support": "cd services/support-service && npm run build",
    "build:integration": "cd services/integration-service && npm run build",
    "build:webhook": "cd services/webhook-service && npm run build",
    "build:marketing": "cd apps/marketing-website && npm run build",
    "build:customer-portal": "cd apps/customer-portal && npm run build",
    "build:admin-portal": "cd apps/admin-portal && npm run build",
    "deploy": "npm run deploy:azure",
    "deploy:azure": "cd infrastructure/azure && terraform apply",
    "agent:create": "node tools/saas-agent.js create",
    "agent:deploy": "node tools/saas-agent.js deploy",
    "agent:configure": "node tools/saas-agent.js configure",
    "marketing:analytics": "node tools/digital-marketing-setup.js analytics",
    "marketing:social": "node tools/digital-marketing-setup.js social",
    "marketing:gmb": "node tools/digital-marketing-setup.js gmb",
    "marketing:google-ads": "node tools/digital-marketing-setup.js google-ads",
    "marketing:meta-ads": "node tools/digital-marketing-setup.js meta-ads",
    "marketing:data-studio": "node tools/digital-marketing-setup.js data-studio",
    "business-plan": "node tools/business-plan-generator.js generate",
    "market-research": "node tools/business-plan-generator.js market-research",
    "financial-projections": "node tools/business-plan-generator.js financial-projections",
    "video-content": "node tools/video-content-generator.js generate",
    "video-scripts": "node tools/video-content-generator.js scripts",
    "video-equipment": "node tools/video-content-generator.js equipment",
    "marketing:setup-all": "node tools/marketing-automation.js setup-all",
    "marketing:seo-audit": "node tools/marketing-automation.js seo-audit",
    "marketing:competitor-analysis": "node tools/marketing-automation.js competitor-analysis",
    "marketing:content-calendar": "node tools/marketing-automation.js content-calendar",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix"
  },
  "keywords": [
    "saas",
    "template",
    "framework",
    "multi-tenant",
    "azure",
    "microservices"
  ],
  "author": "SaaS Template Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.29.1",
    "uuid": "^9.0.1",
    "applicationinsights": "^2.9.0",
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "chalk": "^5.0.0",
    "commander": "^11.0.0",
    "concurrently": "^8.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "googleapis": "^164.1.0",
    "inquirer": "^9.0.0",
    "ora": "^7.0.0"
  }
}
